@model IEnumerable<ParveenEvent.Models.tbl_Reg>

@{
    ViewBag.Title = "RegisteredReport";
    Layout = "~/Views/Shared/_RLayout.cshtml";
}

<h2>Total Registered Customers</h2>


<table class="table" id="TblTotalregusers">
    <thead class="thead-light">
        <tr>
            <th scope="col">
                ID
            </th>
            <th scope="col">
                NAME
            </th>
            <th scope="col">
                Mobile
            </th>
            <th scope="col">
                Company
            </th>
            <th scope="col">
                Designation
            </th>
            <th scope="col">
                Email
            </th>


        </tr>
    </thead>

    @foreach (var item in Model)
    {
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Cus_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cus_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cus_Mobile)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cus_Company)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cus_Occupation)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Cus_Email)
                </td>

            </tr>
        }
    }

</table>
@*Floating button*@
<div class="zoom">
    <a class="zoom-fab zoom-btn-larged printregrpt" id="zoomBtn" title="Print" ><i class="material-icons">&#xE8AD;</i></a>
